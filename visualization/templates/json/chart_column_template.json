{
    chart: {
        type: $CHART_TYPE
    },
    title: {
        text: $TITLE_TEXT
    },
    xAxis: {
        categories: $XAXIS_CATEGORIES
    },
    legend: {
        enabled: false
    },
    series: [{
        name: $SERIES_NAME,
        data: $SERIES_DATA,
        dataLabels: {
            enabled: true,
            rotation: -90,
            color: '#FFFFFF',
            align: 'right',
            x: 4,
            y: 10,
            style: {
                fontSize: '13px',
                fontFamily: 'Verdana, sans-serif',
                textShadow: '0 0 3px black'
            }
        }
    }]
}
